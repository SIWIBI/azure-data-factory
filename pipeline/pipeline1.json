{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Load stageTransactions",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "POST",
							"additionalHeaders": "Content-Type: application/json",
							"requestBody": "{ \"query\" : \"query {\n  journalHeaders(last: 1000, filter: { modifiedAt_gte:\\\"2022-05-22\\\"}) {\n    edges {\n      node {\n        dbId\n      }\n    }\n  }\n}\"}",
							"requestTimeout": ""
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['data']['journalHeaders']['edges'][0]['node']['dbId']"
								},
								"sink": {
									"name": "_JournalHeaderKey",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['data']['journalHeaders']['edges']"
					}
				},
				"inputs": [
					{
						"referenceName": "JSONEXL",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "dsStageJournalHeaders",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}